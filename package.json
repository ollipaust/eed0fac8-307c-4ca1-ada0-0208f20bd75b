{
  "name": "vennew",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"pnpm run dev:css\" \"pnpm run dev-remix\"",
    "dev-remix": "remix dev --manual",
    "start": "netlify serve",
    "produce": "pnpm run build && pnpm start",
    "typecheck": "tsc -b",
    "build:css": "node-sass ./app/ui/styles -o ./app/ui/styles --output-style compressed",
    "dev:css": "pnpm run build:css && node-sass -rw ./app/ui/styles -o ./app/ui/styles"
  },
  "dependencies": {
    "@remix-run/node": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "algoliasearch": "^4.20.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-instantsearch": "^7.3.0"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}